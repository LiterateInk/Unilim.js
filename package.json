{
  "name": "unilim",
  "version": "1.0.0",
  "description": "A purrfect API wrapper for Unilim.",
  "author": "LiterateInk <contact@literate.ink> (https://literate.ink)",
  "license": "GPL-3.0-or-later",
  "homepage": "https://unilim.docs.literate.ink/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiterateInk/Unilim.js.git"
  },
  "bugs": {
    "url": "https://github.com/LiterateInk/Unilim.js/issues"
  },
  "workspaces": [
    ".",
    "examples"
  ],
  "keywords": [
    "unilim",
    "biome",
    "api",
    "wrapper",
    "student",
    "university"
  ],
  "sideEffects": false,
  "exports": {
    "./cas": {
      "import": {
        "types": "./dist/cas/index.d.mts",
        "default": "./dist/cas/index.mjs"
      },
      "require": {
        "types": "./dist/cas/index.d.ts",
        "default": "./dist/cas/index.js"
      }
    },
    "./biome": {
      "import": {
        "types": "./dist/biome/index.d.mts",
        "default": "./dist/biome/index.mjs"
      },
      "require": {
        "types": "./dist/biome/index.d.ts",
        "default": "./dist/biome/index.js"
      }
    },
    "./iut/cs/timetable": {
      "import": {
        "types": "./dist/iut/cs/timetable/index.d.mts",
        "default": "./dist/iut/cs/timetable/index.mjs"
      },
      "require": {
        "types": "./dist/iut/cs/timetable/index.d.ts",
        "default": "./dist/iut/cs/timetable/index.js"
      }
    },
    "./iut/signatures": {
      "import": {
        "types": "./dist/iut/signatures/index.d.mts",
        "default": "./dist/iut/signatures/index.mjs"
      },
      "require": {
        "types": "./dist/iut/signatures/index.d.ts",
        "default": "./dist/iut/signatures/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "example": "bun --env-file examples/.env",
    "build": "tsdown"
  },
  "dependencies": {
    "@literate.ink/pdf-inspector": "^1.0.0",
    "@scure/base": "^2.0.0",
    "cheerio": "^1.1.2",
    "luxon": "^3.7.2",
    "schwi": "^0.8.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.23",
    "@types/luxon": "^3.7.1",
    "@vexcited/eslint-config": "^0.1.1",
    "eslint": "^9.37.0",
    "jiti": "^2.6.1",
    "terser": "^5.44.0",
    "tsdown": "^0.15.6",
    "typedoc": "^0.28.13",
    "typedoc-plugin-remove-3dots-default-value": "^0.1.0",
    "typescript": "^5.9.3"
  }
}
