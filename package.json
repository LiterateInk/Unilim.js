{
  "name": "pawnilim",
  "version": "0.2.0",
  "description": "A purrfect API wrapper for Unilim.",
  "repository": "https://github.com/LiterateInk/Pawnilim",
  "sideEffects": false,
  "exports": {
    "./biome": {
      "types": "./dist/biome/index.d.ts",
      "require": "./dist/biome/index.js",
      "import": "./dist/biome/index.mjs"
    },
    "./cas": {
      "types": "./dist/cas/index.d.ts",
      "require": "./dist/cas/index.js",
      "import": "./dist/cas/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "checks": "tsc && eslint ."
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "unilim",
    "biome",
    "api",
    "wrapper",
    "student",
    "university"
  ],
  "author": "LiterateInk <contact@literate.ink> (https://literate.ink)",
  "homepage": "https://docs.literate.ink/pawnilim",
  "bugs": {
    "url": "https://github.com/LiterateInk/Pawnilim/issues"
  },
  "license": "GPL-3.0-or-later",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/parser": "^7.13.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "terser": "^5.31.1",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@literate.ink/utilities": "1.0.0-11713907881.1"
  }
}
